@model IEnumerable<Game_Step.Models.Game>

@{
    ViewBag.Title = "Control Panel";
}
<a asp-action="Create" asp-controller="Game">Create</a>

@foreach (var item in Model)
{
    <div class="col-md-12">
        Name - @item.Article
        <a asp-action="Read" asp-controller="Game" asp-route-id="@item.Id">Read</a>
        <a asp-action="Update" asp-controller="Game" asp-route-id="@item.Id">Update</a>
        <a asp-action="Delete" asp-controller="Game" asp-route-id="@item.Id">Delete</a>
    </div>
}

