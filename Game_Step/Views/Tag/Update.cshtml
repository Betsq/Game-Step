@model Game_Step.Models.Tag

@{
    ViewBag.Title = "Update " + Model.Name;
}

<div class="container">
    <form asp-action="Update" asp-controller="Tag" method="post">
        <input type="text" asp-for="Name" placeholder="Name" />
        <input type="text" asp-for="NameParam" placeholder="Name param" />

        <input type="submit" value="Update" />
    </form>
</div>